<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="Primary" Color="#68BA1C"/>
    <SolidColorBrush x:Key="Secondary" Color="#ff212121"/>
    <SolidColorBrush x:Key="Background" Color="#EAEAEA"/>
    <SolidColorBrush x:Key="OnPrimary" Color="White"/>
    
    <LinearGradientBrush x:Key="DarkBrush" StartPoint="0,0" EndPoint="0,1" Opacity="0,3">
      <GradientBrush.GradientStops>
        <GradientStopCollection>
          <GradientStop Color="Transparent" Offset="0.0"/>
          <GradientStop Color="Black" Offset="1.0"/>
        </GradientStopCollection>
      </GradientBrush.GradientStops>
    </LinearGradientBrush>
    
    <SolidColorBrush x:Key="DisabledBackgroundBrush" Color="Black" Opacity="0,15"/>
    <SolidColorBrush x:Key="DisabledBorderBrush" Color="Black" Opacity="0,5"/>
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="Black" Opacity="0,5"/>
</ResourceDictionary>